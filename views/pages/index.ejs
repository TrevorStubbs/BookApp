<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./layouts/head.ejs') %>
</head>
<body>
  <header>
  </header>
  <main>
    <p><a href="<%= link %>">searches</a></p>
  <% bookshelfResults.forEach( book => { %>
      <div>
        <img src="<%= book.image_url %>" alt="<%= book.title %>">
        <h2><%= book.title %></h2>
        <h3><%= book.author %></h3>
      </div>
      <form action="books/<%= book.id %>">
        <button>Details</button>
      </form>
  <% }) %>
 </main> 
 <footer>
 </footer>
</body>
</html>
